# Firebase Configuration (Production Mode)
# For production deployment on Blackbox server
# Firebase will work in development mode (console logging) without credentials
FIREBASE_PROJECT_ID=rideshare-hub-production

# Session Configuration
# Secure session secret for production
SESSION_SECRET=blackbox-rideshare-secure-session-key-2026-production

# Application Configuration
NODE_ENV=production
PORT=5000
HOST=0.0.0.0

# CORS Configuration
CORS_ORIGIN=*

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
